#include <stdio.h>

int main(){
    int a = 1;
    while(a<=5){
        printf("Torre %ix direita\n", a);
        a++;
    }
    int b = 1;
    while(b<=5){
        printf("Bispo %ix direita cima\n", b);
        b++;
    }
    int c = 1;
    while(c<=8){
        printf("Rainha %ix esquerda\n", c);
        c++;
    }
    for(int d=1; d <=2; d++){
        printf("Cavalo %ix baixo\n", d);
    }
    int e=1;
        while(e <=1){
            printf("Cavalo %ix esquerda\n", e);
            e++;
        }
        
    
    
    return 0;
}
